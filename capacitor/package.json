{
  "name": "hairstyle-app",
  "version": "1.0.0",
  "description": "AI Hair Style Simulator",
  "main": "index.js",
  "scripts": {
    "build": "cp -r ../frontend/* www/",
    "cap:init": "npx cap init 'Hair Style Simulator' com.hairstyle.app --web-dir www",
    "cap:add:android": "npx cap add android",
    "cap:add:ios": "npx cap add ios",
    "cap:sync": "npm run build && npx cap sync",
    "cap:open:android": "npx cap open android",
    "cap:open:ios": "npx cap open ios"
  },
  "keywords": ["hairstyle", "ai", "simulator"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@capacitor/android": "^6.0.0",
    "@capacitor/core": "^6.0.0",
    "@capacitor/ios": "^6.0.0",
    "@capacitor/camera": "^6.0.0",
    "@capacitor/filesystem": "^6.0.0",
    "@capacitor/share": "^6.0.0"
  },
  "devDependencies": {
    "@capacitor/cli": "^6.0.0"
  }
}
