# Google Cloud設定
GCP_PROJECT_ID=your-project-id
GCP_LOCATION=us-central1
GOOGLE_APPLICATION_CREDENTIALS_JSON={"type": "service_account", ...}

# サーバー設定
PORT=8080
FLASK_DEBUG=false

# API認証（本番では必ず設定）
API_KEY=your-secret-api-key

# CORS設定（カンマ区切りで複数指定可）
ALLOWED_ORIGINS=https://your-app-domain.com

# レート制限（1分あたりの最大リクエスト数）
RATE_LIMIT_MAX=30
